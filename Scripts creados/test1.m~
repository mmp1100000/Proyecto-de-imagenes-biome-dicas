d = uigetdir;
path = d;
    s = size(path);
    if(path(s(2))~='/')
        path=[path,'/'];
    end
    d = dir(path);
    dirLen = size(d);
    dirLen = dirLen(1);
    
    for file = 1:dirLen
        if(d(file).isdir == 0)
            info = dicominfo(strcat(path,d(file).name));
            %dicomVector = [dicomVector, struct(d(file).name,{info.SliceLocation})];
            dicomVector(file) = struct(d(file).name,info.SliceLocation);
        end
    end 

